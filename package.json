{
  "name": "house-stack-template",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "author": "drewjs (https://drewjs.com)",
  "type": "module",
  "scripts": {
    "build": "remix build",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:push": "drizzle-kit push:sqlite",
    "db:seed": "node --loader esbuild-register/loader -r esbuild-register ./app/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "remix dev --manual",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "pnpm db:push && pnpm db:seed",
    "start": "binode -r dotenv/config -- @remix-run/serve:remix-serve ./build/index.js",
    "start:mocks": "binode -r dotenv/config -r ./mocks/index.cjs -- @remix-run/serve:remix-serve ./build/index.js",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "test:e2e:run": "cross-env CI=true playwright test",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@libsql/client": "^0.3.5",
    "@remix-run/css-bundle": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.0.0",
    "binode": "^1.0.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "isbot": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@flydotio/dockerfile": "latest",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@playwright/test": "^1.39.0",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/bcryptjs": "^2.4.4",
    "@types/node": "^18.18.5",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.51.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "happy-dom": "^12.9.1",
    "msw": "^1.3.2",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/coverage",
    "/playwright-report"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
